{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": [ 1, "never" ],
    "import/no-extraneous-dependencies": [ 0, {
      "devDependencies": false
    }],
    "react/jsx-filename-extension": [ 1, {
      "extensions": [ ".js", ".jsx" ]
    }],
    "max-len": 0,
    "no-console": 0,
    "strict": 0
  }
}
